<?xml version="1.0"?>
<root name="test_case">

<type name="simpleType">
	<parameter name="typeParam" type="integer" min="0" max="10"/>
	<node name="toto" nb_instances="2">
	<parameter name="paramT" type="integer" min="0" max="10"/>
	<parameter name="paramT2" type="integer" min="0" max="10"/>
	<parameter name="paramT3" type="integer" min="0" max="10"/>
	</node>
	<constraint name="cType" expressions="simpleType\toto[0]\paramT EQ 10" />
