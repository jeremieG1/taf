	<parameter name="paramN" type="integer" min="0" max="10"/>
	<constraint name="cNode" expressions="typeInstantiation[0]\paramN EQ 10" />
	<constraint name="cNode1" expressions="typeInstantiation[0]\toto[i]\paramT3 EQ 10" types="forall" quantifiers="i" ranges="[0,1]" />
</node>

<node name="typeInstantiation2" type="simpleType">
	<parameter name="paramN2" type="integer" min="0" max="10"/>
	<constraint name="cNode2" expressions="typeInstantiation2[0]\paramN2 EQ 10" />
	<constraint name="cNode22" expressions="typeInstantiation2[0]\toto[i]\paramT3 EQ 10" types="forall" quantifiers="i" ranges="[0,..\toto.nb_instances+1]" />
</node>

</root>
